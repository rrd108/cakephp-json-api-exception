{
  "name": "rrd108/cakephp-json-api-exception",
  "description": "JsonApiException plugin for CakePHP",
  "version": "0.4.0",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "cakephp/cakephp": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.3"
  },
  "autoload": {
    "psr-4": {
      "JsonApiException\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonApiException\\Test\\": "tests/",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
    }
  }
}
